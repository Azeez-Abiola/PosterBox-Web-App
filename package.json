    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.37",
    "tailwindcss": "^3.4.1",
